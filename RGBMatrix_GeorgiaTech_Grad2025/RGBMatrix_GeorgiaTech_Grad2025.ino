/**
 * Print my message
 */
void displayMESSAGE(){
    int ypos = 20;    
      // Draw big scrolly text on top
    matrix->setTextColor(matrix->ColorHSV(hue, 255, 255, true));
    matrix->setCursor(textX, ypos);
    matrix->print(F2(str));
  
    // Move text left (w/wrap), increase hue
    if((--textX) < textMin) textX = 14;
    hue += 7;
    if(hue >= 1536) hue -= 1536;
}


